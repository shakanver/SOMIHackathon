{
    "manifest_version": 2,
    "name": "Epilepsy Warning System",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"
}